      <div>
        <h4> Product List </h4>
        <table class="table table-hover">
          <thead>
            <tr>
              <th>Name</th>
              <th>Price</th>
              <th>Image</th>
            </tr>
          </thead>
          <tbody>
            {{#each products}}
             <tr>
                <td headers="name">{{name}}</td>
                <td headers="price">{{price}}</td>
                <td headers="url"><img src={{url}} width="50px" height="50px" alt={{url}}</td>
             </tr>
            {{/each}}
          </tbody>
        </table>
      </div>